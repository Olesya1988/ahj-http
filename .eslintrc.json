{
    "extends": "airbnb-base",
    "env": {
       "es6": true,
       "browser": true,
       "jest": true
    },
    "rules": {
      "no-restricted-syntax": [
        "error",
        "LabeledStatement",
        "WithStatement"
      ],
      "no-plusplus": 0,
      "no-useless-return": 0,
      "no-restricted-globals": 0,
      "func-names": 0
    }
  }